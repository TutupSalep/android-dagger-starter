<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.rakasettya.daggerstarter">

  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.CAMERA"/>
  <uses-permission android:name="android.permission.VIBRATE"/>

  <application
    android:allowBackup="true"
    android:icon="@drawable/ic_logo"
    android:label="@string/app_name"
    android:name="com.rakasettya.daggerstarter.MyApplication"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name="com.rakasettya.daggerstarter.apps.splash.SplashActivity"
      android:theme="@style/AppThemeFullScreen">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name="com.rakasettya.daggerstarter.apps.main.MainActivity"/>
    <activity android:name="com.rakasettya.daggerstarter.apps.barcode.BarcodeActivity">
    </activity>
    <activity android:name="com.rakasettya.daggerstarter.apps.signin.SignInActivity">

    </activity>
  </application>

</manifest>